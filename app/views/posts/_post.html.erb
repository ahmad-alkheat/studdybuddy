<% @posts.each do |post| %>
	<div id="post">
		<%= post.user_name%> is looking for a study partner for
		<%= post.course_name %>
		<%= post.course_number %>
		<%= image_tag show_image(post.user) %>
		<%= link_to "view user", profiles_path(post.user_id) %>
		<% if  post.user == current_user%>
			<%= link_to "Edit", edit_post_path(post) %>
			<%= link_to "Delete", post_path(post), method: :DELETE, data: { confirm: 'Are you sure?' }  %>
		<% end %>
	</div>
<% end%>