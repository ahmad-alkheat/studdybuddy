<% @posts.each do |post| %>
	<div id="post">
		<%= post.course_name %>
		<%= post.course_number %>
		<%= link_to "Edit", edit_post_path(post) %>
		<%= link_to "Delete", post_path(post), method: :DELETE, data: { confirm: 'Are you sure?' }  %>
	</div>
	<% end%>

<%= link_to "New request", new_post_path %>